{
  "rewrites": [
    {
      "source": "/Build/:file.framework.js",
      "has": [
        { "type": "header", "key": "Accept-Encoding", "value": "(?i)(^|.*,\\s*)br(\\s*,.*|$)" }
      ],
      "destination": "/Build/:file.framework.js.br"
    },
    {
      "source": "/Build/:file.data",
      "has": [
        { "type": "header", "key": "Accept-Encoding", "value": "(?i)(^|.*,\\s*)br(\\s*,.*|$)" }
      ],
      "destination": "/Build/:file.data.br"
    },
    {
      "source": "/Build/:file.wasm",
      "has": [
        { "type": "header", "key": "Accept-Encoding", "value": "(?i)(^|.*,\\s*)br(\\s*,.*|$)" }
      ],
      "destination": "/Build/:file.wasm.br"
    }
  ],
  "headers": [
    {
      "source": "/Build/:file.loader.js",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript; charset=utf-8" },
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/Build/:file.framework.js.br",
      "headers": [
        { "key": "Content-Encoding", "value": "br" },
        { "key": "Content-Type", "value": "application/javascript; charset=utf-8" },
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/Build/:file.data.br",
      "headers": [
        { "key": "Content-Encoding", "value": "br" },
        { "key": "Content-Type", "value": "application/octet-stream" },
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/Build/:file.wasm.br",
      "headers": [
        { "key": "Content-Encoding", "value": "br" },
        { "key": "Content-Type", "value": "application/wasm" },
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/Build/:file.framework.js",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript; charset=utf-8" },
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/Build/:file.data",
      "headers": [
        { "key": "Content-Type", "value": "application/octet-stream" },
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/Build/:file.wasm",
      "headers": [
        { "key": "Content-Type", "value": "application/wasm" },
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
